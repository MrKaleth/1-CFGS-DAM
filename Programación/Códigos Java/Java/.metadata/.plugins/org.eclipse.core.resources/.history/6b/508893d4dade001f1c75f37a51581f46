package ejercicioAnimales;

public class Animal {
	String tipo;
	public String nombre;
	private int edad;

	boolean caminar() {
		return true;
	}
	
	private void comer() {
		System.out.println("Tengo: " + edad + "Y estoy comiendo");
	}
	
	public void darComer() {
		if(tipo.equals("Perro")) {
			comer();
		}
	}
}