private void maulla() {
	System.out.println("Miauuuuu");
}
public boolean maullar() {
	boolean maullando = true;
	maulla();
	return maullando;
}

private void ronronea() {
	System.out.println("Miauuuuu");
}
public boolean ronronear() {
	boolean ronroneo = true;
	ronronea();
	return ronroneo;
}
}
